@using EvalEngine.Domain.Entities
@using EvalEngine.UI.Controllers
<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="en" lang="en">
<head>
<meta http-equiv="X-UA-Compatible" content="IE=9; IE=8; IE=7; IE=EDGE" />
<meta http-equiv="Content-type" content="text/html; charset=ISO-8859-1">
<title>@ViewBag.Title</title>	
<link rel="shortcut icon" type="image/x-icon" href="@Url.Content("~/Content/inc/img/ee.ico")" />
<link rel="stylesheet" href="@Url.Content("~/Content/inc/css/reset-fonts-grids.css")" type="text/css" />
<link rel="stylesheet" href="@Url.Content("~/Content/inc/css/styles.css")" type="text/css" />
<link rel="stylesheet" href="@Url.Content("~/Content/inc/css/jquery-ui.css")" type="text/css" />
<!--[if lt IE 9]><link rel="stylesheet" type="text/css" href="inc/css/ie8.css"><![endif]-->
<link href='https://fonts.googleapis.com/css?family=Open+Sans:400,300,600,400italic,700' rel='stylesheet' type='text/css'>
<script src="@Url.Content("~/Scripts/jquery-1.5.1.min.js")" type="text/javascript"></script>
<script src="@Url.Content("~/Content/inc/js/jquery.tablesorter.js")" type="text/javascript"></script>
<script src="@Url.Content("~/Content/inc/js/d3.v3.min.js")" type="text/javascript"></script>
<script src="@Url.Content("~/Content/inc/js/dimple.v2.1.6.min.js")" type="text/javascript"></script>
<script src="@Url.Content("~/Content/inc/js/jquery-ui-1.8.22.custom.min.js")" type="text/javascript"></script>
<script src="@Url.Content("~/Scripts/jquery.validate.min.js")" type="text/javascript"></script>
<script src="@Url.Content("~/Scripts/jquery.validate.unobtrusive.min.js")" type="text/javascript"></script>
<script src="@Url.Content("~/Scripts/jquery.unobtrusive-ajax.min.js")" type="text/javascript"></script>
<script type="text/javascript" src="@Url.Content("~/Scripts/jquery.ui.datepicker.validation.min.js")"></script>
</head>

<body>

<div class="bar">
	@if (User.Identity.IsAuthenticated)
	{
	<div class="acct-nav">
		<ul class="acct-links">
			<li><a href="@this.Url.Action("LogOff", "Account")">Logout</a></li>
			@if (HttpContext.Current.User.IsInRole(Constants.SiteAdministratorRole) || HttpContext.Current.User.IsInRole(Constants.ProjectAdminRole) || HttpContext.Current.User.IsInRole(Constants.StateAdminRole)){
       <li class="space">|</li>
			<li><a href="@this.Url.Action("Users", "Account")">Manage Accounts</a></li> }
			<li class="space">|</li>
			<li><a href="@this.Url.Action("MyAccount", "Account")">My Account</a></li>
			<li class="space">|</li>
            <li>Welcome, @AccountController.UserName(HttpContext.Current.User.Identity.Name)</li>
		</ul>
	</div><!--end acct-nav-->
    }
</div><!--end bar-->

<div class="header">
	<div class="header-nav">
		<ul class="header-links">
			<li><a href="@this.Url.Action("Index", "Home")">Home</a></li>
			<li class="space">|</li>
		  <li><a href="@this.Url.Action("Library", "Analysis")">My Library</a></li>
			<li class="space">|</li>
		  <li><span style="color: rgb(129, 147, 158); font-weight:600;">Technical Documentation</span></li>
			<li class="space">|</li>
		  <li><a href="@this.Url.Action("About", "Home")">About</a></li>
			<li class="space">|</li>
		  <li><a href="@this.Url.Action("Help", "Home")">Help</a></li>
		</ul>
	</div><!--end header-links-->
	<div class="header-line">&nbsp;</div>
	<div class="ee-logo"><img src="@Url.Content("~/Content/inc/img/evaluation_engine.png")" alt="Evaluation Engine" width="329" height="61" /></div>
</div><!--end header-->

<div class="container">

	<div class="content_wrapper">

	<!-- stuff after here is MVC -->

		<div id="main">
			@RenderBody()
		</div>
		<!-- MVC above here -->

	</div><!--end content_wrapper-->


</div><!--end container-->
<div class="popup" style="position:absolute;height:200px;width:300px;display:none;"></div>
    <script type="text/javascript">
        $(document).ready(function () {
            
        });
  </script> 
</body>
</html>